page {
  height: 100%;
  width: 100%;
  background: #f4f4f4;
}
/* to */
.order-info {
  margin-top: 20rpx;
  background: #fff;
  height: auto;
  overflow: hidden;
  padding: 0 30rpx;
}

.order-header {
  padding: 0 30rpx 25rpx;
  background-color: #ffffff;
  margin-bottom: 10rpx;
}
.order-header .order-status {
  display: flex;
  align-items: center;
  height: 100rpx
}

.order-header .order-stateTxt {
  font-size: 32rpx;
  font-weight: bold;
  color: #CF1116;
}
.order-header .order-member {
  margin-bottom: 8rpx;
  font-size: 34rpx;
  font-weight: bold;
}
.order-member text {
  margin-right: 15rpx;
}
.order-member .icondizhi {
  font-size: 36rpx;
}

.order-header .order-site {
  margin-left: 50rpx;
  color: #999999;
}

.order-header .order-icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}
.order-pay {
  background: url(http://129.204.122.146:61245/hy3pm8wy2l2r9x8q47p7.png)no-repeat center center;
  background-size: 100%;
}
.order-ship {
  background: url(http://129.204.122.146:61245/89naxo7ufluhmwtjesc4.png)no-repeat center center;
  background-size: 100%;
}
.order-finish {
  background: url(http://129.204.122.146:61245/m961tjditba2fte665dg.png)no-repeat center center;
  background-size: 100%;
}
.order-close {
  background: url(http://129.204.122.146:61245/1p186dvnaj75a4qbifkh.png)no-repeat center center;
  background-size: 100%;
}
.order-home {
  background: url(http://129.204.122.146:61245/dcaw2rxoc5g9mafe7bx6.png)no-repeat center center;
  background-size: 100%;
}

.order-info .order-title {
  position: relative;
  padding-left: 15rpx;
  margin: 30rpx 0;
}
.order-info .order-title::before {
  content: '';
  position: absolute;
  left: 0;
  top: 3rpx;
  width: 7rpx;
  height: 35rpx;
  background-color: #CF1116;
}
.order-info .order-item {
  padding: 5rpx 10rpx 20rpx 10rpx;
}
.order-info .label {
  margin-right: 40rpx;
  width: 100rpx;
  color: #666666;
}
.order-info .content {
  font-size: 28rpx;
}
.order-info .order-copy {
  margin-left: 40rpx;
  display: inline-block;
  color: #CF1116;
}
/* end */
.item {
  padding-left: 31.25rpx;
  height: 42.5rpx;
  padding-bottom: 12.5rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  color: #666;
}

.item-c {
  margin-left: 31.25rpx;
  border-top: 1px solid #f4f4f4;
  height: 103rpx;
  line-height: 103rpx;
}

.item-c .l {
  float: left;
}

.item-c .r {
  height: 103rpx;
  float: right;
  display: flex;
  align-items: center;
  padding-right: 16rpx;
}

.item-c .r .btn {
  float: right;
}

.item-c .cost {
  color: #b4282d;
}

.item-c .btn {
  line-height: 66rpx;
  border-radius: 5rpx;
  text-align: center;
  margin: 0 15rpx;
  padding: 0 20rpx;
  height: 66rpx;
}

.item-c .btn.active {
  background: #b4282d;
  color: #fff;
}

.order-goods {
  margin-top: 20rpx;
  background: #fff;
}

.order-goods .h {
  height: 93.75rpx;
  line-height: 93.75rpx;
  margin-left: 31.25rpx;
  border-bottom: 1px solid #f4f4f4;
  padding-right: 31.25rpx;
}

.order-goods .h .label {
  float: left;
  padding-left: 15rpx;
  font-size: 30rpx;
  color: #333;
  position: relative;
}
.order-goods .h .label::before {
  content: '';
  position: absolute;
  left: 0;
  top: 30rpx;
  width: 7rpx;
  height: 35rpx;
  background-color: #CF1116;
}

.order-goods .h .status {
  float: right;
  font-size: 30rpx;
  color: #b4282d;
}

.order-goods .item {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 192rpx;
  padding-right: 31.25rpx;
}

.order-goods .item:last-child {
  border-bottom: none;
}

.order-goods .item .img {
  height: 145.83rpx;
  width: 145.83rpx;
  background: #f4f4f4;
}

.order-goods .item .img image {
  height: 145.83rpx;
  width: 145.83rpx;
}

.order-goods .item .info {
  flex: 1;
  margin: 0 20rpx;
}
.order-goods .item .r {
  text-align: right;
  height: 145.83rpx;
}
.order-goods .item .t {
  margin-bottom: 20rpx;
}

.order-goods .item .t .name {
  color:  #333;
  font-size:  28rpx;
  display:  -webkit-box;
  overflow:  hidden;
  text-overflow:  ellipsis;
  word-wrap:  break-word;
  -webkit-line-clamp:  2;
  -webkit-box-orient:  vertical;
  width: 100%;
  white-space: normal !important;
}

.order-goods .item .r .number {
  margin-top: 3rpx;
  font-size: 26rpx;
  color: #999999;
}

.order-goods .item .attr {
  color: #aaa;
  font-size: 24rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  width: 100%;
  white-space: normal !important
}
.order-goods .item-progress {
  margin: 0 30rpx 10rpx;
  border-radius: 20rpx;
  background-color: #ebeef5;
  overflow: hidden;
  position: relative;
  vertical-align: middle;
  height: 2rpx;
}

.order-goods .item-progress .inner{
  height: 100%;
  text-align: right;
  border-radius: 20rpx;
  line-height: 1;
  color: #fff;
  white-space: nowrap;
  transition: width .6s ease;
  background-color: #CF1116;
 }

.order-goods .item .price {
  display: block;
  float: left;
  height: 30rpx;
  line-height: 30rpx;
  color: #333;
  font-size: 30rpx;
}

.order-goods .item .btn {
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 5rpx;
  text-align: center;
  display: block;
  float: right;
  margin: 0 15rpx;
  padding: 0 20rpx;
}

.order-goods .item .btn.active {
  background: #b4282d;
  color: #fff;
}

.order-bottom {
  margin-top: 15rpx;
  padding: 0 30rpx 20rpx;
  height: auto;
  overflow: hidden;
  background: #fff;
  text-align: right;
}

.order-bottom .address {
  height: 128rpx;
  padding-top: 25rpx;
  border-bottom: 1px solid #f4f4f4;
}

.order-bottom .address .t {
  height: 35rpx;
  line-height: 35rpx;
  margin-bottom: 7.5rpx;
}

.order-bottom .address .name {
  display: inline-block;
  height: 35rpx;
  width: 140rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}

.order-bottom .address .mobile {
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}

.order-bottom .address .b {
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}

.order-bottom .total {
  /* height: 95rpx; */
  /* border-bottom: 1px solid #f4f4f4; */
}

.order-bottom .total .t {
  font-size: 24rpx;
  color: #666666;
  height: 30rpx;
  line-height: 30rpx;
  margin-bottom: 7.5rpx;
}

.order-bottom .total .t text{
  display: inline-block;
  min-width: 170rpx;
}
.order-bottom .total .active {
  color: #CF1116;
}
.order-bottom .total .label {
  width: 150rpx;
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}

.order-bottom .total .txt {
  flex: 1;
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
}
.order-bottom .opr {
  margin-top: 25rpx;
  overflow: hidden;
}
.order-bottom .contact-warp {
  position: relative;
  margin-left: 20rpx;
  padding: 10rpx;
  float: right;
  border: 1rpx solid #dedede;
  width: 200rpx;
  text-align: center;
  border-radius: 30rpx;
} 

.order-bottom .pay-fee {
  height: 81rpx;
  line-height: 81rpx;
}

.order-bottom .pay-fee .label {
  display: inline-block;
  width: 140rpx;
  color: #b4282d;
}

.order-bottom .pay-fee .txt {
  display: inline-block;
  width: 140rpx;
  color: #b4282d;
}

.order-express {
  margin-top: 20rpx;
  width: 100%;
  height: 100rpx;
  background: #fff;
}

.order-express .title {
  float: left;
  margin-bottom: 20rpx;
  padding: 10rpx;
}

.order-express .ti {
  float: right;
  width: 52rpx;
  height: 52rpx;
  margin-right: 16rpx;
  margin-top: 28rpx;
}

.order-express .t {
  font-size: 29rpx;
  margin-left: 10.25rpx;
  color: #a78845;
}

.order-express .b {
  font-size: 29rpx;
  margin-left: 10.25rpx;
  color: #a78845;
}

.order-express .traces {
  padding: 17.5rpx;
  background: #fff;
  border-bottom: 1rpx solid #f1e6cdcc;
}

.order-express .trace {
  padding-bottom: 17.5rpx;
  padding-top: 17.5rpx;
  background: #fff;
}

.order-express .acceptTime {
  margin-top: 20rpx;
  margin-right: 40rpx;
  text-align: right;
  font-size: 26rpx;
}

.order-express .acceptStation {
  font-size: 26rpx;
}
